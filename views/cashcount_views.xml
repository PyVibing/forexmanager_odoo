<?xml version="1.0" encoding="UTF-8"?>

<odoo>

    <!-- List view of forexmanager.cashcount -->
    <record id="forexmanager_cashcount_list_view" model="ir.ui.view">
        <field name="name">ForexManager cashcount list view</field>
        <field name="model">forexmanager.cashcount</field>
        <field name="arch" type="xml">
            <list create="0" duplicate="0">
                <field name="id" width="5" groups="forexmanager.group_forexmanager_admin"/>
                <field name="name" width="600" groups="forexmanager.group_forexmanager_admin"/>
                <field name="balance" width="50" groups="forexmanager.group_forexmanager_admin"/>
                <field name="currency_id" width="200" groups="forexmanager.group_forexmanager_admin"/>     
                <field name="workcenter_id" width="100" groups="forexmanager.group_forexmanager_admin"/>
                <field name="desk_id" width="500" groups="forexmanager.group_forexmanager_admin"/>
            </list>
        </field>
    </record>

    <!-- Action of forexmanager.cashcount -->
    <record id="forexmanager_config_cashcounts_action" model="ir.actions.act_window">
        <field name="name">ADMINISTRAR INVENTARIOS</field>
        <field name="res_model">forexmanager.cashcount</field>
        <field name="view_mode">list</field>
    </record>
    
</odoo>